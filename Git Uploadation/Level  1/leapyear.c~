#include<stdio.h>
void main()
{	
	int i,j,k,l,n;
	int yr;
	printf("enter a year");
	scanf("%d",&yr);
	if(yr%4==0)
	{
		if(yr%100==0)
		{
			if(yr%400==0)
			printf("%d is leap year",yr);
			else
			printf("%d is not leap year",yr);
		}
		else
			printf("%d is leap year",yr);
	}	
	else
	printf("%d is not leap year ",yr);
	
}
